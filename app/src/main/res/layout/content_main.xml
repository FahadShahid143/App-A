        tools:layout_editor_absoluteX="136dp"
        tools:layout_editor_absoluteY="384dp"
        android:onClick="buttonClickOne"/>

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="App-A"
