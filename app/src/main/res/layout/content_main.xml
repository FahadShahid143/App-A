<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.fahadshahid.app_a.MainActivity"
    tools:showIn="@layout/activity_main">

    <TextView
        android:id="@+id/textView"
        android:layout_width="153dp"
        android:layout_height="53dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="62dp"
        android:layout_marginTop="9dp"
        android:layout_x="118dp"
        android:layout_y="7dp"
        android:text="App-A"
        android:textSize="40dp"
        tools:layout_editor_absoluteX="136dp"
        tools:layout_editor_absoluteY="47dp" />

    <Button
        android:id="@+id/button4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/textView"
        android:layout_marginEnd="7dp"
        android:layout_marginTop="30dp"
        android:layout_x="103dp"
        android:layout_y="116dp"
        android:onClick="buttonClickOne"
        android:text="Start Activity 1"
        tools:layout_editor_absoluteX="120dp"
        tools:layout_editor_absoluteY="168dp" />

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/button4"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="58dp"
        android:layout_x="111dp"
        android:layout_y="280dp"
        android:onClick="buttonClickTwo"
        android:text="Start Activity 2"
        tools:layout_editor_absoluteX="120dp"
        tools:layout_editor_absoluteY="414dp" />
</AbsoluteLayout>
